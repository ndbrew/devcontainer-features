{

  "install_plugins_on_plain_ubuntu": {
    "image": "ubuntu:22.04",
    "features": {
      "zsh-plugins": {
        "plugins": "npm dogesay",
        "omzPlugins": "https://github.com/txstc55/dogesay"
      }
    }
  },
  "install_plugins": {
    "image": "mcr.microsoft.com/devcontainers/base:debian",
    "features": {
      "zsh-plugins": {
        "plugins": "npm dogesay",
        "omzPlugins": "https://github.com/txstc55/dogesay"
      }
    }
  },
  "multiple-omz-plugins": {
    "image": "mcr.microsoft.com/devcontainers/base:debian",
    "features": {
      "zsh-plugins": {
        "plugins": "npm git node F-Sy-H zsh-autocomplete",
        "omzPlugins": "https://github.com/z-shell/F-Sy-H https://github.com/marlonrichert/zsh-autocomplete"
      }
    }
  }
}